{
	"env": {
		"node": true
	},
	"extends": ["prettier", "plugin:@typescript-eslint/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json",
		"sourceType": "module"
	},
	"ignorePatterns": ["demo/**/*"],
	"plugins": ["@typescript-eslint"],
	"rules": {
		"@typescript-eslint/no-explicit-any": "off"
	}
}
